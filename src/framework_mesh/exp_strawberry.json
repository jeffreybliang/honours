{
    "project": "Flowchart",
    "name": "strawberry",
    "description": "sky, sphere to strawberry",
    "src_mesh": "sphere",
    "target_meshes": [
        "rstrawberry"
    ],
    "views": {
        "rstrawberry": {
            "mode": "manual",
            "view_idx": [0,1,2,3,5,6,7,8,11],
            "num_views": 9
        }
    },
    "training": {
    "n_iters": 100,
    "lr": 1e-5,
    "momentum": 0.8
    },
    "gradient": {
        "smoothing": true,
        "method": "jacobi",
        "k": 4,
        "constrained": false,
        "debug": true
    },
    "verbose": true,
    "vis": {
        "enabled": true,
        "frequency": 2
    },
    "wandb": true
}
